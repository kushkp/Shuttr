<%=auth_token%>

<label for="user_email">Email</label>
<input type="text"
       id="user_email"
       name="user[email]"
       value="<%= @user.email if @user%>">

<br>

<label for="user_password">Password</label>
<input type="password"
       id="user_password"
       name="user[password]"
       value="<%= @user.password if @user%>">
